{
  "name": "sandbox-prisma-adapter-aurora",
  "version": "1.0.0",
  "description": "A sandbox project to test the Prisma Aurora Adapter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rayk47/sandbox-prisma-adapter-aurora.git"
  },
  "author": "Raymond Kelly <raymondjaykay@gmail.com.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rayk47/sandbox-prisma-adapter-aurora/issues"
  },
  "homepage": "https://github.com/rayk47/sandbox-prisma-adapter-aurora#readme",
  "scripts": {
    "build": "tsc",
    "lint": "eslint",
    "build-prisma-client": "npx prisma generate --schema=./src/lib/handlers/prisma/schema.prisma",
    "build-prisma-migrations": "npx prisma migrate dev --schema=./src/lib/handlers/prisma/schema.prisma",
    "deploy": "npm run build && npx aws-cdk deploy --all --require-approval never",
    "deploy-lambdas": "npm run build && npx aws-cdk deploy PrismaDataAPIlambdas --force",
    "destroy": "npx aws-cdk destroy prisma-rds-data-api* --force",
    "bootstrap": "npx aws-cdk bootstrap"
  },
  "bin": {
    "aws-lambda": "bin/main.js"
  },
  "dependencies": {
    "@aws-sdk/client-rds-data": "^3.600.0",
    "@raymondjkelly/aurora-prisma-adapter": "1.0.2",
    "aws-cdk-lib": "2.147.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.5",
    "source-map-support": "^0.5.21",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.4",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "typescript-eslint": "^8.5.0"
  }
}