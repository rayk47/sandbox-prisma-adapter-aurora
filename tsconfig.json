{
  "compilerOptions": {
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "es2022",
    "allowJs": true,
    "resolveJsonModule": true,
    "moduleDetection": "force",
    "isolatedModules": true,
    "verbatimModuleSyntax": false,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "module": "commonjs",
    "noEmit": true,
    "lib": [
      "es2022"
    ]
  },
  "exclude": [
    "src/lib/services/database",
    "cdk.out"
  ]
}